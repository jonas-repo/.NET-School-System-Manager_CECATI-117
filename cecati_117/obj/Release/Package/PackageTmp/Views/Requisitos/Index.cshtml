@model IEnumerable<cecati_117.Models.RequisitosInscripcion>
@{
    ViewBag.Title = "Requisitos de inscripción";
}
<div class="breadcrumb-wrapper">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-xs-12 col-sm-6">
                <h2 class="title">Requisitos de inscripción</h2>
            </div>
            <div class="col-lg-6 col-md-6 col-xs-12 col-sm-6">
                <div class="breadcrumbs pull-right">
                    <ul>
                        <li>Estas aquí:</li>
                        <li><a href="@Url.Action("Index","Home")">Inicio</a></li>
                        <li>Nosotros</li>
                        <li>Requisitos de inscripción</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="divider"></div>
<!-- 3 Column Services -->
<div class="services">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h3 class="title"></h3>
            </div>
            <div class="clearfix"></div>

            @foreach (var requisito in Model)
            {
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 bottom-pad">
                    <div class="content-box ch-item">
                        <div class="ch-info-wrap">
                            <div class="ch-info">
                                <div class="ch-info-front ch-img-1"><i class="fa @requisito.RequisitosInscripcion_icono"></i></div>
                                <div class="ch-info-back">
                                    <i class="fa @requisito.RequisitosInscripcion_icono"></i>
                                </div>
                            </div>
                        </div>
                        <div class="content-box-info">
                            <h4>@requisito.RequisitosInscripcion_titulo</h4>
                            @Html.Raw(requisito.RequisitosInscripcion_descripcion)
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
<div class="divider"></div>

